{
	a = 12;
	H = 1;
	emc = 0.577;
	max_a = 0;
	while(a < 1000000000,
		if(numdiv(a) > max_a,
			max_a = numdiv(a);
                               i = 1;
                               l = divisors(a);
                               sum_d = 0;
                              printf("++++++++++++++++++++++++++++++++++++++++++++++\n");                     
                               while(i < max_a-1,
                                       sum_d = sum_d + l[i]/(l[i+1]*l[max_a-i+1]);
                                       print("\t\t\t",l[i],"/",(l[i+1]*l[max_a-i+1]),"\t",1.0*l[i]/(l[i+1]*l[max_a-i+1]));
                                       i++
                               );
                              printf("------------------------------------------------------------------------------------------------
----------------------------------------------\n%d\t\t\t(Sum) %.4f\t(Avg) %.4f\n",a,sum_d,sum_d/(max_a-1));
                       
		);
		a++
	);
}
