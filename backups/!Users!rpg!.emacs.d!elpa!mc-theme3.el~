
(defun mc-theme3 ()
  "Theme generated by Sweyla: http://themes.sweyla.com/seed/168161/"
  (interactive)
  (color-theme-install
   '(sweyla168161
     ((background-color . "#FBFFFF")
      (foreground-color . "#000000")
      (background-mode . bright)
      (border-color . "#C8C8C8")
      (cursor-color . "#000000")
      (mouse-color . "#C8C8C8"))
     (mode-line ((t (:foreground "#000000" :background "#C8C8C8"))))
     (region ((t (:background "#C8C8C8"))))

     (font-lock-comment-face ((t (:foreground "#004CF9"))))
     (font-lock-constant-face ((t (:foreground "#30C49B"))))
     (font-lock-builtin-face ((t (:foreground "#878096"))))
     (font-lock-function-name-face ((t (:foreground "#367DF6"))))
     (font-lock-variable-name-face ((t (:foreground "#53CA6A"))))
     (font-lock-keyword-face ((t (:foreground "#211AA4"))))
     (font-lock-string-face ((t (:foreground "#2AC47E"))))
     (font-lock-doc-string-face ((t (:foreground "#2AC47E"))))
     (font-lock-type-face ((t (:foreground "#3C8CC4"))))
     )))

(provide 'mc-theme3)
