(defun mc-theme2 ()
  (interactive)
  (color-theme-install
   '(mc-theme2
      ((background-color . "#ffffff")
      (background-mode . light)
      (border-color . "#000000")
      (cursor-color . "#333333")
      (foreground-color . "#090a0b")
      (mouse-color . "black"))
     (fringe ((t (:background "#000000"))))
     (mode-line ((t (:foreground "#f5f4f4" :background "#353841"))))
     (region ((t (:background "#dddee4"))))
     (font-lock-builtin-face ((t (:foreground "#1b3cde"))))
     (font-lock-comment-face ((t (:foreground "#2f4c29"))))
     (font-lock-function-name-face ((t (:foreground "#090a0b"))))
     (font-lock-keyword-face ((t (:foreground "#0d4a87"))))
     (font-lock-string-face ((t (:foreground "#090a0b"))))
     (font-lock-type-face ((t (:foreground"#266719"))))
     (font-lock-variable-name-face ((t (:foreground "#062e56"))))
     (minibuffer-prompt ((t (:foreground "#090a0b" :bold t))))
     (font-lock-warning-face ((t (:foreground "Red" :bold t))))
     )))
(provide 'mc-theme2)
